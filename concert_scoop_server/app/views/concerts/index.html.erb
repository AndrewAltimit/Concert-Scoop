<h1>Listing concerts</h1>

<table>
  <tr>
    <th>Name</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Start time</th>
    <th>Twitter tag</th>
    <th>Facebook page</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @concerts.each do |concert| %>
  <tr>
    <td><%= concert.name %></td>
    <td><%= concert.city %></td>
    <td><%= concert.state %></td>
    <td><%= concert.zip %></td>
    <td><%= concert.start_time %></td>
    <td><%= concert.twitter_tag %></td>
    <td><%= concert.facebook_page %></td>
    <td><%= link_to 'Show', concert %></td>
    <td><%= link_to 'Edit', edit_concert_path(concert) %></td>
    <td><%= link_to 'Destroy', concert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

